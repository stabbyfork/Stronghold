{
	"dependencies": {
		"@sequelize/mysql": "^7.0.0-alpha.46",
		"@sequelize/sqlite3": "^7.0.0-alpha.46",
		"ajv": "^8.17.1",
		"discord-api-types": "^0.38.16",
		"discord.js": "^14.21.0",
		"lodash": "^4.17.21",
		"parse-duration": "^2.1.4",
		"sequelize": "^7.0.0-alpha.9",
		"typescript": "^5.8.3"
	},
	"type": "module",
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@types/ms": "^2.1.0",
		"@types/node": "^24.5.2",
		"@typescript-eslint/parser": "^8.38.0",
		"eslint": "^9.31.0",
		"globals": "^16.3.0",
		"jiti": "^2.4.2",
		"json-schema-to-typescript": "^15.0.4",
		"nodemon": "^3.1.10",
		"prettier": "^3.6.2",
		"tsx": "^4.20.3",
		"typescript-eslint": "^8.37.0"
	},
	"scripts": {
		"start:production": "pm2 start ./ecosystem.config.cjs --time --env production",
		"start": "pm2 start ./ecosystem.config.cjs --time",
		"stop": "pm2 stop ./ecosystem.config.cjs",
		"restart": "pm2 restart ./ecosystem.config.cjs --time",
		"build:production": "npm run build:schema && npm run build:index && npm run build:register prod && npm run compile && echo 'Build complete'",
		"build:dev": "npm run build:schema && npm run build:index && npm run build:register dev && npm run compile && echo 'Build complete'",
		"compile": "npx tsc",
		"build:index": "npx tsx scripts/cmd-index.ts",
		"build:register": "npx tsx scripts/cmd-register.ts",
		"build:schema": "npx json2ts -i ./*.schema.json > src/schema-types.d.ts",
		"preinstall": "npm ls --global pm2@>=6.0.8 || npm install --global pm2@latest",
		"compileAndRestart": "npm run compile && pm2 flush && npm restart",
		"logs": "pm2 logs"
	},
	"version": "1.0.0"
}
